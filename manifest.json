{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "PopCorn Bookmarks",
  "description": "AI Bookmarks to help your future self remember why you saved this page",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "action": {
    "default_popup": "js/index.html",
    "default_title": "PopCorn Bookmark"
  },
  "host_permissions": ["https://twitter.com/", "https://chat.openai.com/*"],
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "cookies",
    "notifications",
    "tabs",
    "activeTab",
    "sidePanel"
  ]
}
